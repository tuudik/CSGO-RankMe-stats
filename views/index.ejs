<!DOCTYPE html>
<html>

<head>
  <% include ./partials/head %>
  <script src="/static/js/overview.js"></script>
  <link rel="apple-touch-icon" sizes="180x180" href="https://info.csgo-server.com/apple-touch-icon.png?v=XBbd3ya7bw">
<link rel="icon" type="image/png" sizes="32x32" href="https://info.csgo-server.com/favicon-32x32.png?v=XBbd3ya7bw">
<link rel="icon" type="image/png" sizes="16x16" href="https://info.csgo-server.com/favicon-16x16.png?v=XBbd3ya7bw">
<link rel="mask-icon" href="https://info.csgo-server.com/safari-pinned-tab.svg?v=XBbd3ya7bw">
<link rel="shortcut icon" href="https://info.csgo-server.com/favicon.ico?v=XBbd3ya7bw">
<meta name="msapplication-TileColor" content="#da532c">
<meta name="theme-color" content="#ffffff">
</head>

<body>

  <% include ./partials/header %>

  <br>

  <div class="container-fluid">
    <% if(global.config.historicalData.enabled) { %>
    <div class="row">
      <div class="col-sm-6">
        <div class="card text-center">
          <div id="score-inc" class="card-body">
            <div class="card-header">
              <h2>Score</h2>
            </div>
            <p class="card-text">

              <canvas id="score-increase-chart"></canvas>

            </p>
            <ol class="two-columns"></ol>
          </div>
        </div>
      </div>
      <div class="col-sm-6">
        <div class="card text-center">
          <div id="kills-inc" class="card-body">
            <div class="card-header">
              <h2>Kills</h2>
            </div>
            <p class="card-text">

              <canvas id="kills-increase-chart"></canvas>

            </p>
            <ol class="two-columns"></ol>
          </div>
        </div>
      </div>
    </div>

    <hr>
    <% } %>

    <div class="card text-center">
      <div class="card-body">
        <div class="card-header">
          <h2>Players</h2>
        </div>
        <p class="card-text">

          <table class="table" id="overview">
            <thead>
              <tr>
                <th>Name</th>
                <th>Steam ID</th>
                <th>Score</th>
                <th>Kill/death ratio</th>
                <th>Headshot %</th>
                <th>Kills</th>
                <th>Deaths</th>
              </tr>
            </thead>
            <tbody>

            </tbody>
          </table>

        </p>
      </div>
    </div>
  </div>
  <script src="/static/js/clipboard.min.js"></script>
  <script src="/static/js/clipboard.addons.js"></script>
</body>

</html>
